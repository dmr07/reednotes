.notes-list
  display block
  // width: 400px
  // min-height: 800px

.liabilityDisclaimer
  font-size: .82em;
  color: rgba(0,0,0,.33);
  padding-bottom: 20px;
  border-bottom: 1px solid #eee;
  margin-bottom: 30px;
  span 
    display: inline-block

.note-title
  font-size: 28px;
  margin: 0px 0 20px;
  background-color: inherit;
  color: #111;
  display: inline-block;
  color: rgba(0,0,0,.84)
  font-family: 'graphik lcg web';
  font-family: 'Garamond Premier Pro';
  font-family: 'Noe Display';
  text-align: left;
  padding-bottom: 10px;
  word-spacing: 0em;
  font-weight: 500;

.note-topic
  background-color: #d7efee !important;
  font-family: "Marat Sans";
  display: inline-block;
  text-transform: uppercase;
  font-weight: 600;
  font-size: 0.74em;
  letter-spacing: .03em;
  color: rgba(0,0,0,.70);
  padding: 2px 5px 0px;
  vertical-align: top;
  margin-left: 8px;
    
.note-body
  font-size: 1em;
  font-family: 'Adobe Caslon Pro';
  font-family: 'CharterITC';
  font-weight: 500;
  color: rgb(60,60,60);
  color: rgba(0,0,0,.76)!important
  padding-bottom: 40px 
  p
    line-height: 1.6em;
  
.note-timestamp
  font-family: 'Marat Sans';
  text-align: left;
  padding: 4px 0 0px;
  color: #3c3830;
  font-size: 0.86em;
  letter-spacing: 0.14em;
  text-transform: uppercase;
  opacity: 0.55;
  margin-bottom: 10px;
  
.note-divider
  width:calc(100% + 100px)
  margin-left: -20px
  height: 1px
  background: #f6f9fa
  margin-bottom: 40px